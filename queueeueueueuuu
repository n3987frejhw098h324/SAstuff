	local localPlayer = game.Players.LocalPlayer
	local localCharacter = localPlayer.Character or localPlayer.CharacterAdded:Wait()
	local hrp = localCharacter:WaitForChild("HumanoidRootPart")

	local POS2_A = Vector3.new(66, 44, 90) -- replace this with your actual POS2_A
	local POS2_B = Vector3.new(-75, 44, 93)
	local TP_SPOT = Vector3.new(67, 44, 99) -- replace this with your actual TP_SPOT

	tweenTo(hrp, POS2_A, 1)

	task.spawn(function()
		while true do
			for _ = 1, 2 do
				task.wait(17)
				tweenTo(hrp, TP_SPOT, 1)
				tweenTo(hrp, POS2_A, 1)
			end
			for _ = 1, 2 do
				task.wait(17)
				tweenTo(hrp, TP_SPOT, 1)
				tweenTo(hrp, POS2_B, 1)
			end
		end
	end)

	return
